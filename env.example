# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
COGNITO_CLIENT_ID=your-client-id
COGNITO_CLIENT_SECRET=your-client-secret

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=nyasc-platform

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Email Configuration (SES)
FROM_EMAIL=hello@nyasc.co
SES_REGION=us-east-1

# JWT Configuration
JWT_SECRET=your-jwt-secret-key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://nyasc.co
NEXTAUTH_URL=https://nyasc.co
NEXTAUTH_SECRET=your-nextauth-secret

# Analytics Configuration
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
POSTHOG_KEY=phc_...
POSTHOG_HOST=https://app.posthog.com

# Search Configuration
MEILISEARCH_URL=https://your-meilisearch-instance.com
MEILISEARCH_API_KEY=your-meilisearch-key

# S3 Configuration
S3_BUCKET_NAME=nyasc-platform-assets
S3_REGION=us-east-1

# Database Configuration (if using additional DB)
DATABASE_URL=postgresql://user:password@localhost:5432/nyasc

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Monitoring Configuration
SENTRY_DSN=https://your-sentry-dsn
LOG_LEVEL=info

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_MONITORING=true
ENABLE_SEARCH=true
ENABLE_EMAIL=true

# Security Configuration
CORS_ORIGIN=https://nyasc.co
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# Development Configuration
NODE_ENV=production
DEBUG=false
